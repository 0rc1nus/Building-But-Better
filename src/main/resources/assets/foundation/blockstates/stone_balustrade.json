{
  "multipart": [
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_post",

        "type=none,axis=y": {"model": "hearth_and_home:block/stone_balustrade_post"},
        "type=top,axis=y": {"model": "hearth_and_home:block/stone_balustrade_top"},
        "type=middle,axis=y": {"model": "hearth_and_home:block/stone_balustrade_middle"},
        "type=bottom,axis=y": {"model": "hearth_and_home:block/stone_balustrade_bottom"}
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side2",
        "uvlock": true
      },
      "when": {
        "north": "low"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side2",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "east": "low"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side",
        "uvlock": false,
        "y": 180
      },
      "when": {
        "south": "low"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side",
        "uvlock": false,
        "y": 270
      },
      "when": {
        "west": "low"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side_tall",
        "uvlock": true
      },
      "when": {
        "north": "tall"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side_tall",
        "uvlock": false,
        "y": 90
      },
      "when": {
        "east": "tall"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side_tall",
        "uvlock": false,
        "y": 180
      },
      "when": {
        "south": "tall"
      }
    },
    {
      "apply": {
        "model": "foundation:block/stone_balustrade_side_tall",
        "uvlock": false,
        "y": 270
      },
      "when": {
        "west": "tall"
      }
    }
  ]
}